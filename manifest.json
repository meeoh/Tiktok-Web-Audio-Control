{
  "name": "Tiktok Audio Control",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Control the audio of the video player on tiktoks webi client",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["https://www.tiktok.com/*/video/*"],
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*/video/*"],
      "js": ["src/index.js"]
    }
  ]
}
