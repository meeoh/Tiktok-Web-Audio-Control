{
  "name": "Tiktok Audio Control",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Control the audio of the video player on tiktoks web client",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions": ["https://www.tiktok.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*"],
      "js": ["src/index.js"]
    }
  ]
}
